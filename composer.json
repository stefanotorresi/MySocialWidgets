{
    "name": "stefanotorresi/my-social-widgets",
    "description": "A Zend Framework 2 module providing view helpers to display widgetized informations retrieved from various social network APIs",
    "type": "library",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "social",
        "widgets"
    ],
    "homepage": "https://github.com/stefanotorresi/MySocialWidgets",
    "authors": [
        {
            "name": "Stefano Torresi",
            "email": "webdev@stefanotorresi.it",
            "homepage": "http://stefanotorresi.it"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zf-commons/zfc-base": "*",
        "zendframework/zend-view": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-cache": "2.*"
    },
    "require-dev": {
        "zendframework/zend-config": "2.*",
        "satooshi/php-coveralls":    "~0.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "MySocialWidgets": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "license": "MIT"
}
